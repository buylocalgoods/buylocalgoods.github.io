          "_name": "shop02",
          "_customHTML": "<section class=\"shop1 amp-shop\" group=\"Shop\" plugins=\"amp-main, amp-lightbox, amp-bind\">\n\t<mbr-parameters>\n\t\t<!-- Block parameters controls (Blue \"Gear\" panel) -->\n\t\t<header>Size</header>\n\t\t<input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"3\">\n\t\t<input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"1\">\n\t\t<header>Show/Hide</header>\n\t\t<input type=\"checkbox\" name=\"showText\" title=\"Show Item Text\" checked=\"\">\n\t\t<!--  -->\n\t\t<header>Card</header>\n\t\t<input type=\"checkbox\" name=\"showText2\" title=\"Show Card Text\" checked=\"\">\n\t\t<input type=\"checkbox\" title=\"Card Subtitle\" name=\"showsubTitle\">\n\t\t<input type=\"checkbox\" title=\"Card Button\" name=\"showButtons\" checked=\"\">\n\t\t<select title=\"Align\" name=\"alignTitle\">\n\t\t\t<option value=\"1\" selected=\"\">Left</option>\n\t\t\t<option value=\"2\">Center</option>\n\t\t\t<option value=\"3\">Right</option>\n\t\t</select>\n\t\t<input type=\"color\" name=\"cardbg2\" title=\"Card 2 BG Color\" value=\"#e7d8d6\">\n\t\t<header>Items</header>\n\n\n\n\n\n\t\t<header condition=\"customStyle\">Custom Styles</header>\n\t\t<input type=\"checkbox\" name=\"customStyles\" title=\"Custom Style\" checked=\"\">\n\n\t\t<header condition=\"customStyles\">Card Color</header>\n\t\t<input type=\"checkbox\" name=\"useColorCard\" title=\"Use Background Card\" condition=\"customStyles\" checked=\"\">\n\t\t<input type=\"color\" name=\"colorCard\" title=\"Color\" value=\"#e7e7e7\" condition=\"useColorCard &amp;&amp; customStyles\">\n\t\t<input type=\"range\" title=\"Opacity\" name=\"opacityCard\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.4\" condition=\"useColorCard &amp;&amp; customStyles\">\n\t\t<input type=\"color\" name=\"fontColor\" title=\"Font Color\" value=\"#000000\" condition=\"customStyles\">\n\t\t<input type=\"color\" name=\"btn